<?import java.lang.*?>
<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.customerui.SQL"
            prefHeight="400.0" prefWidth="600.0" style="-fx-background-color:fef6c7" >
    <headerText>

    </headerText>
    <content>
        <VBox maxWidth="150" style="-fx-background-color:fef6c7"  >
            <GridPane vgap="10"  >
                <Label text="Loan Number* " GridPane.rowIndex="0" GridPane.columnIndex="0"/>
                <TextField fx:id="editoption1" onKeyTyped="#customerid" GridPane.rowIndex="0"  GridPane.columnIndex="1" />
                <Button text="FETCH" GridPane.rowIndex="0" GridPane.columnIndex="2"
                        style="-fx-background-color:darkseagreen" onAction="#inquiryLoan"/>

                 <Text text="Name" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
                <TextField fx:id="inquiryname" disable="true" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
                <Text text="Phone No" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
                <TextField  fx:id="inquiryphone"  disable="true" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
                <Text text="City" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
                <TextField fx:id="inquirycity" disable="true" GridPane.rowIndex="3" GridPane.columnIndex="1"/>

                <Text text="Interest" GridPane.rowIndex="4"  GridPane.columnIndex="0"/>
                <TextField fx:id="inquiryint"  disable="true" GridPane.rowIndex="4" GridPane.columnIndex="1"/>

                <Text text="End Date" GridPane.rowIndex="5" GridPane.columnIndex="0"/>
                <TextField fx:id="inquiryend" disable="true" GridPane.rowIndex="5" GridPane.columnIndex="1"/>

                <Text text="Colletral Value" GridPane.rowIndex="4" GridPane.columnIndex="2"/>
                <TextField fx:id="collet" disable="true" GridPane.rowIndex="4" GridPane.columnIndex="3"/>


                <Text text="Loan Amount" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
                <TextField fx:id="inquiryloanamtoun" disable="true" GridPane.rowIndex="1" GridPane.columnIndex="3"/>

                <Text text="Rate/Month" GridPane.rowIndex="2" GridPane.columnIndex="2"/>
                <TextField fx:id="rateinquiry" disable="true" GridPane.rowIndex="2" GridPane.columnIndex="3"/>

                <Text text="Date of Loan" GridPane.rowIndex="3" GridPane.columnIndex="2"/>
                <TextField fx:id="dateofinquiry" disable="true" GridPane.rowIndex="3" GridPane.columnIndex="3"/>

                <Text text="Closed" GridPane.rowIndex="5" GridPane.columnIndex="2"/>
                <TextField fx:id="closeinquiry" disable="true"  GridPane.rowIndex="5" GridPane.columnIndex="3"/>

                <Button fx:id="close" text="CLOSE" GridPane.rowIndex="6" GridPane.columnIndex="3"
                        style="-fx-background-color:#d5f4e6"
                        onAction="#closeButtonAction"/>


















            </GridPane>
        </VBox>
    </content>



</DialogPane>

